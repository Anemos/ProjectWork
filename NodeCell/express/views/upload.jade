extends layout
block content
  #wrapper
    #display
      include userbar
  div.container
    div.contr <h2> Drag and Drop your images to 'Drop Area' (up to 5 files at a time, size - under 256MB)</h2>
    div.upload_form_cont
      #dropArea Drop Area
      div.info
        div Files left: <span id="count">0</span>
        div Destination url: <input id="url" value="/upload"/>
        h2 Result:
        #result
        <canvas width="500" height="20"></canvas>
  <script src="/javascripts/script.js"></script>
  include footer