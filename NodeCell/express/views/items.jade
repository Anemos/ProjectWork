extends layout

block content
  #wrapper
    #display
      include userbar
      div.container
        div.contr <h2> Drag and Drop your images to 'Drop Area' (up to 5 files at a time, size - under 256kb)</h2>
        div.upload_form_cont
          div.list_form_cont  
            -for (var id in items)
              - var item = items[id]
              div
                a(href='/item/#{id}') 파일명 : #{item.name} - 파일크기 : #{item.size} - 수정일시 : #{item.atime}
      
    include footer